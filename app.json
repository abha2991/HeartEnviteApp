{
  "expo": {
    "name": "HeartEnviteApp",
    "slug": "HeartEnviteApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.HeartEnviteApp",
      "buildNumber": "2.0.0",
      "bitcode": "Debug",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to be converted to pdf",
        "LSApplicationQueriesSchemes": ["tez", "phonepe", "paytmmp"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.HeartEnviteApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7e2e642c-b44f-4f00-b201-0a1a662715e0"
      }
    }
  }
}
